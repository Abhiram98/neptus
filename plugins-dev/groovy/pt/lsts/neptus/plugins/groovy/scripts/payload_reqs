def plan = new Plan("Plan-w-Payload")
def home = new Location(41.185242, -8.704803)

plan.goTo id:"payload-goto", location:home, payload:[name: "Sidescan", active: true]

// set speed units to use
plan.speed.units = Speed.Units.METERS_PS
plan.speed.value = 1.2

plan.description = "This is a PlanSpecification generated by a Groovy builder"
plan.setVehicles "lauv-noptilus-3"
plan.addToConsole console