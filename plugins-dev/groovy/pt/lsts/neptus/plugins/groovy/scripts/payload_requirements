
def plan = new Plan("NVL-Plan")
List<Location> marks = new ArrayList<>()

locations.each {
marks.add new Location(it.getLatitudeRads(),it.getLongitudeRads())
}

// set speed units to use
plan.speed.units = Speed.Units.METERS_PS
plan.speed.value = 1.2

marks.eachWithIndex {
    it,i->

		plan.goTo id:"payload-goto"+i, location:it, payload:[[name: "Sidescan",  Range: 50],[name: "Ranger"]]
}



plan.description = "This is a PlanSpecification generated by a Groovy builder"
plan.setVehicles "lauv-noptilus-2"
plan.addToConsole console //----> from console Binding
