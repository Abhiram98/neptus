def sortedlocations = locations.sort {it.getId()} //from bindings

ArrayList<Location> pois  = new ArrayList<>()

sortedlocations.each {
    	println it.getId() 
        //AbstractElement[] mark = MapGroup.getMapGroupInstance(console.getMission()).getObjectsFromMap(it)
        def loc = new Location(it.getLatitudeRads(),it.getLongitudeRads())
        pois.add loc 
    }
def plan = new Plan ("Travel-POIs")

    pois.each {plan.goTo location:it}
    plan.addToConsole(console)
    
