title "Control Surge"

axis "Velocity x, Desired (m/s)"
axis 1, "Control Output"

vel_x = "EstimatedState.u" 
plot "EstimatedState.u": vel_x

desired_x = "DesiredVelocity.Path Control.u" 
plot "DesiredVelocity.Path Control.u": desired_x

desired_ctrl = "DesiredControl.Horizontal Plane Control.x" 
plot 1, ["DesiredControl.x": desired_ctrl]
