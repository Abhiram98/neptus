title "Control Heading"

axis "Heading, Desired (Â°)"
axis 1, "Control Output"


addQuery "EstimatedState.psi"
heading = apply("Heading", "EstimatedState.psi", toDegrees, normalizeAngleDegrees180)
plot heading

addQuery "DesiredHeading.Path Control.value"
desired_h = apply("DesiredHeading", "DesiredHeading.Path Control.value", toDegrees)
plot desired_h

desired_ctrl = "DesiredControl.Horizontal Plane Control.n" 
plot 1, ["DesiredControl.n": desired_ctrl]