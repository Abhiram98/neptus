<?xml version="1.0" encoding="UTF-8"?>
<config format="1" version="2017.01.0 (mission/rep17,54a3730,dirty)" system="lauv-xtreme-2-aux" i18n="pt_PT">
  <section name="Power Supervisor" name-i18n="Supervisor de Energia">
    <param name="Entity Label">
      <name-i18n>Nome da Entidade</name-i18n>
      <type>string</type>
      <visibility>developer</visibility>
      <scope>global</scope>
      <default>Power Supervisor</default>
      <units/>
      <desc>Nome principal da entidade</desc>
    </param>
    <param name="Execution Priority">
      <name-i18n>Prioridade de Execução</name-i18n>
      <type>integer</type>
      <visibility>developer</visibility>
      <scope>global</scope>
      <default>10</default>
      <units/>
      <desc>Prioridade de execução</desc>
    </param>
    <param name="Activation Time">
      <name-i18n>Tempo de Ativação</name-i18n>
      <type>integer</type>
      <visibility>developer</visibility>
      <scope>global</scope>
      <default>0</default>
      <units/>
      <desc/>
    </param>
    <param name="Deactivation Time">
      <name-i18n>Tempo de Desativação</name-i18n>
      <type>integer</type>
      <visibility>developer</visibility>
      <scope>global</scope>
      <default>0</default>
      <units/>
      <desc/>
    </param>
    <param name="Debug Level">
      <name-i18n>Nível de Depuração</name-i18n>
      <type>string</type>
      <visibility>developer</visibility>
      <scope>global</scope>
      <default>None</default>
      <units/>
      <desc/>
      <values>None, Debug, Trace, Spew</values>
      <values-i18n>None, Debug, Trace, Spew</values-i18n>
    </param>
    <param name="Main Power Channel">
      <name-i18n>Main Power Channel</name-i18n>
      <type>string</type>
      <visibility>developer</visibility>
      <scope>global</scope>
      <default>System</default>
      <units/>
      <desc>Name of the main power channel</desc>
    </param>
    <param name="Command - On Power Down">
      <name-i18n>Command - On Power Down</name-i18n>
      <type>string</type>
      <visibility>developer</visibility>
      <scope>global</scope>
      <default>services syslog stop &amp;&amp; mount -o remount,ro /opt</default>
      <units/>
      <desc>Command to execute before powering down the system</desc>
    </param>
    <param name="Command - On Power Down Abort">
      <name-i18n>Command - On Power Down Abort</name-i18n>
      <type>string</type>
      <visibility>developer</visibility>
      <scope>global</scope>
      <default>mount -o remount,rw /opt &amp;&amp; services syslog restart</default>
      <units/>
      <desc>Command to execute when the power down sequence is aborted</desc>
    </param>
    <param name="Slave System Names">
      <name-i18n>Slave System Names</name-i18n>
      <type>list:string</type>
      <visibility>developer</visibility>
      <scope>global</scope>
      <default/>
      <units/>
      <desc>Name of the slave systems</desc>
    </param>
  </section>
  <section name="TCP to Master" name-i18n="TCP para o Mestre">
    <param name="Entity Label">
      <name-i18n>Nome da Entidade</name-i18n>
      <type>string</type>
      <visibility>developer</visibility>
      <scope>global</scope>
      <default>TCP to Master</default>
      <units/>
      <desc>Nome principal da entidade</desc>
    </param>
    <param name="Execution Priority">
      <name-i18n>Prioridade de Execução</name-i18n>
      <type>integer</type>
      <visibility>developer</visibility>
      <scope>global</scope>
      <default>10</default>
      <units/>
      <desc>Prioridade de execução</desc>
    </param>
    <param name="Activation Time">
      <name-i18n>Tempo de Ativação</name-i18n>
      <type>integer</type>
      <visibility>developer</visibility>
      <scope>global</scope>
      <default>0</default>
      <units/>
      <desc/>
    </param>
    <param name="Deactivation Time">
      <name-i18n>Tempo de Desativação</name-i18n>
      <type>integer</type>
      <visibility>developer</visibility>
      <scope>global</scope>
      <default>0</default>
      <units/>
      <desc/>
    </param>
    <param name="Debug Level">
      <name-i18n>Nível de Depuração</name-i18n>
      <type>string</type>
      <visibility>developer</visibility>
      <scope>global</scope>
      <default>None</default>
      <units/>
      <desc/>
      <values>None, Debug, Trace, Spew</values>
      <values-i18n>None, Debug, Trace, Spew</values-i18n>
    </param>
    <param name="Transports">
      <name-i18n>Transports</name-i18n>
      <type>list:string</type>
      <visibility>developer</visibility>
      <scope>global</scope>
      <default>Heartbeat, LoggingControl, PowerChannelControl, PowerChannelState, EntityState, EntityActivationState, EntityInfo, EntityParameters</default>
      <units/>
      <desc>Messages to transport</desc>
    </param>
    <param name="Rate Limiters">
      <name-i18n>Rate Limiters</name-i18n>
      <type>list:string</type>
      <visibility>developer</visibility>
      <scope>global</scope>
      <default/>
      <units/>
      <desc>List of &lt;Message&gt;:&lt;Frequency&gt;</desc>
    </param>
    <param name="Filtered Entities">
      <name-i18n>Filtered Entities</name-i18n>
      <type>list:string</type>
      <visibility>developer</visibility>
      <scope>global</scope>
      <default/>
      <units/>
      <desc>List of &lt;Message&gt;:&lt;Entity&gt;+&lt;Entity&gt; that define the source entities allowed to pass message of a specific message type.</desc>
    </param>
    <param name="Trace - Incoming Messages">
      <name-i18n>Trace - Incoming Messages</name-i18n>
      <type>boolean</type>
      <visibility>developer</visibility>
      <scope>global</scope>
      <default>false</default>
      <units/>
      <desc>Enable verbose output regarding incoming messages</desc>
    </param>
    <param name="Trace - Outgoing Messages">
      <name-i18n>Trace - Outgoing Messages</name-i18n>
      <type>boolean</type>
      <visibility>developer</visibility>
      <scope>global</scope>
      <default>false</default>
      <units/>
      <desc>Enable verbose output regarding outgoing messages</desc>
    </param>
    <param name="Server - Address">
      <name-i18n>Server - Address</name-i18n>
      <type>ipv4-address</type>
      <visibility>developer</visibility>
      <scope>global</scope>
      <default>10.0.10.50</default>
      <units/>
      <desc>Remote server address</desc>
    </param>
    <param name="Server - Port">
      <name-i18n>Server - Port</name-i18n>
      <type>integer</type>
      <visibility>developer</visibility>
      <scope>global</scope>
      <default>9999</default>
      <units/>
      <desc>Remote server port</desc>
    </param>
  </section>
</config>
