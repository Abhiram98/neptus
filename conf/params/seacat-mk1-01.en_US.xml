  <section name="Acoms" name-i18n="Acoms">
    <param name="Auto Send">
      <name-i18n>Auto Send</name-i18n>
      <type>boolean</type>
      <visibility>user</visibility>
      <scope>maneuver</scope>
      <default>false</default>
      <units/>
      <desc>Unchecked sets to passive mode, checked sets into active mode</desc>
    </param>
    <param name="Repetitions">
      <name-i18n>Repetitions</name-i18n>
      <type>integer</type>
      <visibility>user</visibility>
      <scope>maneuver</scope>
      <default>0</default>
      <units></units>
      <desc></desc>
      <min>0</min>
      <max>5</max>
      <values-if>
        <param>Auto Send</param>
        <equals>false</equals>
        <values>0</values>
      </values-if>
      <values-if>
        <param>Auto Send</param>
        <equals>true</equals>
        <values>1..5</values>
      </values-if>
    </param>
    <param name="In Curves Only">
      <name-i18n>In Curves Only</name-i18n>
      <type>boolean</type>
      <visibility>user</visibility>
      <scope>maneuver</scope>
      <default>false</default>
      <units/>
      <desc>Transmits only on curves</desc>
    </param>
    <param name="Interval">
      <name-i18n>Interval</name-i18n>
      <type>integer</type>
      <visibility>user</visibility>
      <scope>maneuver</scope>
      <default>1</default>
      <units>s</units>
      <desc></desc>
      <min>-1</min>
      <max>60</max>
      <values-if>
        <param>In Curves Only</param>
        <equals>true</equals>
        <values>-1</values>
      </values-if>
      <values-if>
        <param>In Curves Only</param>
        <equals>false</equals>
        <values>1..60</values>
      </values-if>
    </param>
  </section>
