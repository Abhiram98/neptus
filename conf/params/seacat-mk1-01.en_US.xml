  <section name="UUVCam" name-i18n="Camera">
    <param name="Active">
      <name-i18n>Active</name-i18n>
      <type>boolean</type>
      <visibility>user</visibility>
      <scope>maneuver</scope>
      <default>false</default>
      <units/>
      <desc>Activates camera</desc>
    </param>
    <param name="Log On Lines Only">
      <name-i18n>Log On Lines Only</name-i18n>
      <type>boolean</type>
      <visibility>user</visibility>
      <scope>maneuver</scope>
      <default>false</default>
      <units/>
      <desc>Deactivates logging on curves in selected maneuvers.</desc>
    </param>
    <param name="Mission Critical">
      <name-i18n>Mission Critical</name-i18n>
      <type>boolean</type>
      <visibility>user</visibility>
      <scope>plan</scope>
      <default>false</default>
      <units/>
    </param>
    <param name="Framerate">
      <name-i18n>Framerate</name-i18n>
      <type>integer</type>
      <visibility>user</visibility>
      <scope>maneuver</scope>
      <default>4</default>
      <units>fps</units>
      <desc>Framerate for high resolution image capturing</desc>
      <min>1</min>
      <max>5</max>
    </param>
    <param name="Show_Video">
      <name-i18n>Show Video</name-i18n>
      <type>boolean</type>
      <visibility>user</visibility>
      <scope>maneuver</scope>
      <default>false</default>
      <units/>
      <desc>Outputs compressed video</desc>
    </param>
    <param name="Gain">
      <name-i18n>Gain</name-i18n>
      <type>integer</type>
      <visibility>user</visibility>
      <scope>maneuver</scope>
      <default>25</default>
      <units>%</units>
      <desc>Sets light sensivity, 0 for auto gain</desc>
      <min>0</min>
      <max>100</max>
      <values>0, 1..100</values>
    </param>
    <param name="Image_Format">
      <name-i18n>Image Format</name-i18n>
      <type>string</type>
      <visibility>user</visibility>
      <scope>maneuver</scope>
      <default>JPG</default>
      <units/>
      <desc></desc>
      <values>JPG, TIFF, PNG, PGM</values>
      <values-i18n>JPEG, TIFF, PNG, PGM</values-i18n>
    </param>
    <param name="Compression">
      <name-i18n>Image Compression</name-i18n>
      <type>string</type>
      <visibility>user</visibility>
      <scope>maneuver</scope>
      <default>NO</default>
      <units/>
      <desc>Recorded images will be compressed</desc>
      <values>NO, AUTO</values>
      <values-i18n>NO, AUTO</values-i18n>
    </param>
    <param name="Shutter_Time">
      <name-i18n>Shutter Time</name-i18n>
      <type>integer</type>
      <visibility>user</visibility>
      <scope>maneuver</scope>
      <default>0</default>
      <units>ms</units>
      <desc>0 for auto</desc>
      <min>0</min>
      <max>52</max>
    </param>
    <param name="Strobe">
      <name-i18n>Strobe</name-i18n>
