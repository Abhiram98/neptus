  <section name="UUVCam" name-i18n="Camera">
    <param name="Active">
      <name-i18n>Active</name-i18n>
      <type>boolean</type>
      <visibility>user</visibility>
      <scope>maneuver</scope>
      <default>false</default>
      <units/>
      <desc>True to activate task, false otherwise</desc>
    </param>
    <param name="Mission Critical">
      <name-i18n>Mission Critical</name-i18n>
      <type>boolean</type>
      <visibility>user</visibility>
      <scope>plan</scope>
      <default>false</default>
      <units/>
    </param>
    <param name="Cam-Mode">
      <name-i18n>Cam-Mode</name-i18n>
      <type>string</type>
      <visibility>user</visibility>
      <scope>maneuver</scope>
      <default>Images</default>
      <units/>
      <desc></desc>
      <values>Images, Video</values>
      <values-i18n>Still Images, Video</values-i18n>
    </param>
    <param name="Framerate">
      <name-i18n>Framerate</name-i18n>
      <type>integer</type>
      <visibility>user</visibility>
      <scope>maneuver</scope>
      <default>4</default>
      <units>fps</units>
      <desc></desc>
      <min>-1</min>
      <max>4</max>
      <values-if>
        <param>Cam-Mode</param>
        <equals>Images</equals>
        <values>1..4</values>
      </values-if>
      <values-if>
        <param>Cam-Mode</param>
        <equals>Video</equals>
        <values>-1</values>
      </values-if>
    </param>
    <param name="Show-Video">
      <name-i18n>Show Video</name-i18n>
      <type>boolean</type>
      <visibility>user</visibility>
      <scope>maneuver</scope>
      <default>false</default>
      <units/>
    </param>
    <param name="Gain">
      <name-i18n>Gain</name-i18n>
      <type>integer</type>
      <visibility>user</visibility>
      <scope>maneuver</scope>
      <default>25</default>
      <units>%</units>
      <desc>0 for auto gain</desc>
      <min>0</min>
      <max>100</max>
      <values>0, 10..100</values>
    </param>
    <param name="Image-Format">
      <name-i18n>Image Format</name-i18n>
      <type>string</type>
      <visibility>user</visibility>
      <scope>maneuver</scope>
      <default>jpeg</default>
      <units/>
      <desc></desc>
      <values>jpeg, png, pgm, ppm</values>
      <values-i18n>jpeg, png, pgm, ppm</values-i18n>
    </param>
    <param name="Image-Compr">
      <name-i18n>Image Compression</name-i18n>
      <type>boolean</type>
      <visibility>user</visibility>
      <scope>maneuver</scope>
      <default>true</default>
      <units/>
    </param>
    <param name="Shutter-Time">
      <name-i18n>Shutter Time</name-i18n>
      <type>integer</type>
      <visibility>user</visibility>
      <scope>maneuver</scope>
      <default>0</default>
      <units>ms</units>
      <desc>0 for auto</desc>
      <min>0</min>
      <max>1000</max>
    </param>
    <param name="Strobe">
      <name-i18n>Strobe</name-i18n>
      <type>boolean</type>
      <visibility>user</visibility>
      <scope>maneuver</scope>
      <default>false</default>
      <units/>
    </param>
    <param name="Intensity">
      <name-i18n>Strobe Intensity</name-i18n>
      <type>integer</type>
      <visibility>user</visibility>
      <scope>maneuver</scope>
      <default>0</default>
      <units>%</units>
      <desc></desc>
      <min>0</min>
      <max>100</max>
    </param>
  </section>
